{
    "name": "madeny/url-shortener",
    "description": "Url Shortener for Laravel 5 with support for Google and Bitly drivers.",
    "keywords": [
        "madeny",
        "laravel",
        "url",
        "shortener",
        "url short",
        "url shortener"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "madeny",
            "email": "me@madeny.me",
            "homepage": "https://madeny.me"
        }
    ],
    "require": {
        "illuminate/support": "5.3.x|5.4.x|5.5.*",
        "illuminate/config":  "5.3.x|5.4.x|5.5.*",
        "mremi/url-shortener": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6",
        "orchestra/testbench": "3.1.x|3.2.x|3.3.x",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "madeny\\UrlShortener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "madeny\\UrlShortener\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
